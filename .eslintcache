(search) {\n    if(search.target.value.length >= 3){\n      this.state.isSearch = true\n      this.props.searchGames(search.target.value) // games action\n      this.props.searchMovies(search.target.value) // movies action\n    }\n\n    if(search.target.value.length == 0){\n      this.setState({isSearch: false }) // state değişimi setState ile gerçekleşir.  => this.state.isSearch = false işe yaramaz!\n    }\n  }\n\n  closeSearch = () => { //setState görmesi için arrowFunction kullanılması gerekli.\n    this.setState({isSearch: false }) \n    this.searchInput.value = \"\"; //ref={(el) => (this.searchInput = el)} \n  }\n\n  render() {\n    return (\n      <div className=\"search w-100\">\n        <div className=\"search-item d-flex align-items-center\">\n          <img className=\"search-icon pl-4\" src={searchIcon} loading=\"lazy\"/>\n          <input type=\"text\" autoFocus placeholder=\"Oyun yada film ara\" ref={(el) => (this.searchInput = el)} onKeyUp={this.searchText.bind(this)}/>\n        </div>\n        <div className={`search-result ${this.state.isSearch ? \"active\" : \"\"}`}>\n          {this.props.searchListGames.length == 0 && this.props.searchListMovie.length == 0 && <h6>Bir sonuç bulunamadı.</h6>}\n          \n          {/* Games search */}\n          {this.props.searchListGames.map(searchItem => \n            <Link to={`/detail/${searchItem.platform}/${searchItem.slug}`} onClick={this.closeSearch} className=\"search-result__item\">\n              <div className=\"d-flex\">\n                <img src={searchItem.poster} alt=\"\"/>\n                <div className=\"d-flex flex-column\">\n                  <h6 className=\"search-result__name\">{searchItem.name} ({searchItem.year})</h6>\n                  <p className=\"search-result__cat\">{searchItem.category}</p>\n                </div>\n              </div>\n              <div className=\"d-flex\">\n                <p className=\"search-item__platform green\">Oyun</p>\n              </div>\n            </Link>\n        )}\n\n        {/* Movie search */}\n        {this.props.searchListMovie.map(searchItem => \n          <Link to={`/detail/${searchItem.platform}/${searchItem.slug}`} onClick={this.closeSearch} className=\"search-result__item\">\n            <div className=\"d-flex\">\n              <img src={searchItem.poster} alt=\"\"/>\n              <div className=\"d-flex flex-column\">\n                <h6 className=\"search-result__name\">{searchItem.name} ({searchItem.year})</h6>\n                <p className=\"search-result__cat\">{searchItem.category}</p>\n              </div>\n            </div>\n            <div className=\"d-flex\">\n            <p className=\"search-item__platform red\">Film</p>\n            </div>\n          </Link>\n        )}\n        </div>\n      </div>\n    )\n  }\n}\n\nconst mapStateToProps = (state) => { //state => state.games --> games bana rootReducerdan geliyor..\n  return {\n    searchListGames: state.games.searchList,\n    searchListMovie: state.movies.searchList\n  } \n}\n\nconst mapDispatchToProps = { \n  searchGames,\n  searchMovies\n}\n\nexport default connect(mapStateToProps,mapDispatchToProps) (search);\n","/Users/sahinzaybak/Desktop/react-movie-app/src/layout/header.jsx",["127","128","129"],"import React, { PureComponent } from 'react'\nimport '../assets/scss/layout/header.scss'\nimport logo from '../assets/images/logo.png';\n//Components\nimport Search from '../components/search.jsx'\n\nclass header extends PureComponent {\n  state = {\n    isDark:false\n  }\n  toggleSwitch = () => { \n    this.setState({isDark: !this.state.isDark})\n    if(!this.state.isDark) document.body.classList.add(\"dark\");\n    else document.body.classList.remove(\"dark\");\n  }\n\n  render() {\n    return (\n      <div className=\"header sticky-top\">\n        <div className=\"custom-container h-100\">\n          <div className=\"d-flex justify-content-between h-100\">\n              <div className=\"header-logo d-none d-md-flex align-items-center\">\n                <img className=\"header-logo\" src={logo} />\n              </div>\n              <div className=\"header-menu__search ml-0 ml-md-5\">\n                <Search />\n              </div>\n              <div className=\"header-menu d-flex align-items-center\">\n                <a className=\"header-menu__link d-none d-md-block\" href=\"#\">Anasayfa</a>\n                <a className=\"header-menu__link  d-none d-md-block\" href=\"#\">Hakkımda</a>\n                <div className=\"custom-control custom-switch\">\n                  <input type=\"checkbox\" className=\"custom-control-input\" checked={this.state.isLight} id=\"customSwitch1\"/>\n                  <label className=\"custom-control-label\" htmlFor=\"customSwitch1\" onClick={this.toggleSwitch}>Dark Mode</label>\n                </div>\n              </div>\n          </div>\n        </div>\n      </div>\n    )\n  }\n}\n\nexport default header;\n",{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":7,"column":1,"nodeType":"136","endLine":27,"endColumn":2},{"ruleId":"134","severity":1,"message":"135","line":8,"column":1,"nodeType":"136","endLine":44,"endColumn":2},{"ruleId":"137","severity":1,"message":"138","line":12,"column":41,"nodeType":"139","messageId":"140","endLine":12,"endColumn":43},{"ruleId":"137","severity":1,"message":"138","line":15,"column":42,"nodeType":"139","messageId":"140","endLine":15,"endColumn":44},{"ruleId":"141","severity":1,"message":"142","line":13,"column":11,"nodeType":"143","endLine":13,"endColumn":115},{"ruleId":"137","severity":1,"message":"138","line":74,"column":43,"nodeType":"139","messageId":"140","endLine":74,"endColumn":45},{"ruleId":"137","severity":1,"message":"138","line":77,"column":44,"nodeType":"139","messageId":"140","endLine":77,"endColumn":46},{"ruleId":"137","severity":1,"message":"138","line":17,"column":26,"nodeType":"139","messageId":"140","endLine":17,"endColumn":2
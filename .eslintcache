pTypes = {\n  movies: PropTypes.object.isRequired\n};\n\nexport default movieList;","/Users/sahinzaybak/Desktop/react-movie-app/src/components/detail.jsx",["121","122","123","124","125","126"],"import React,{useState} from 'react'\nimport { CircularProgressbar, buildStyles } from 'react-circular-progressbar';\nimport 'react-circular-progressbar/dist/styles.css';\nimport ReactPlayer from 'react-player'\nimport {Link} from 'react-router-dom';\nimport ContentLoader from 'react-content-loader'\n\n//Images\nimport muted from '../assets/images/volume-x.svg';\nimport play from '../assets/images/play.svg';\nimport up from '../assets/images/chevrons-up.svg';\nimport close from '../assets/images/close.svg';\nimport back from '../assets/images/arrow-left.svg';\n\n//Component\nimport EditMovie from '../components/editMovie'\nimport EditGames from '../components/editGame'\n\nconst MovieDetail = ({ detailInfo }) => { //no props => ({movies})\nconst volumeTrailer = 1;\nconst volumePosterTrailer = 0.8;\n\nconst [playingPoster, setPlayPoster] = useState(true) //poster trailer\nconst [trailerMuted, setTrailerMuted] = useState(true) //arka plan video ses\nconst [isTrailer, setTrailer] = useState(false) //trailer izlemek istiyor mu?\nconst [isMute, setIsMute] = useState(false) //trailerdan çıkınca mute aktif mi?\nconst [isEdit, setIsEdit] = useState(false) //trailerdan çıkınca mute aktif mi?\n \nfunction toggleMute(){\n  setPlayPoster(!playingPoster);\n  if(playingPoster) setIsMute(true) // isMute = true\n  else setIsMute(false)\n}\n\nfunction isMutePoster(){\n  if(isMute) setPlayPoster(false) // playingPoster = false\n  else setPlayPoster(true)\n}\n\n//Mobile arka plan trailer için.\nfunction bodyAddClass(){\n  document.body.classList.add(\"overflow-hidden\");\n}\n\nfunction bodyRemoveClass(){\n  document.body.classList.remove(\"overflow-hidden\");\n}\n\nreturn (\n  <div className={`${isTrailer ? \"activeTrailer\" : \"\"}`}> \n    <div className={`movie-detail__backdrop ${isEdit ? \"active\" : \"\"}`}>\n       <ReactPlayer volume={volumeTrailer} loop={true} playing={true} muted={trailerMuted}  url={detailInfo.trailer} loading=\"lazy\"/>\n    </div>\n      <div className={`movie-detail__wrp ${isEdit ? \"active-edit\" : \"\"}`}>\n        <div className=\"custom-container\">\n          {!detailInfo.title &&\n            <div className=\"load-skeleton-detail\">\n              <ContentLoader viewBox=\"0 0 900 500\" width={900} height={500} backgroundColor=\"black\" speed=\"1.2\"\n              foregroundColor=\"#404040\">\n                  <rect x=\"42\" y=\"0.93\" rx=\"5\" ry=\"5\" width=\"280.55\" height=\"410.59\" />\n                  <rect x=\"380.84\" y=\"30.67\" rx=\"0\" ry=\"0\" width=\"500.72\" height=\"20.12\" />\n                  <rect x=\"380.84\" y=\"70.67\" rx=\"0\" ry=\"0\" width=\"300\" height=\"9\" />\n                  <rect x=\"380.84\" y=\"100.67\" rx=\"0\" ry=\"0\" width=\"500.72\" height=\"9.12\" />\n                  <rect x=\"380.84\" y=\"140.67\" rx=\"0\" ry=\"0\" width=\"300\" height=\"9\" />\n                  <rect x=\"380.84\" y=\"180.67\" rx=\"0\" ry=\"0\" width=\"300\" height=\"9\" />\n              </ContentLoader>\n            </div>\n          }\n      {detailInfo.title && \n          <div className={`movie-detail__content ${isEdit ? \"active\" : \"\"}`}>\n            <div className=\"row\">\n              <div className=\"col-md-3\">\n                <Link to={\"/\"} className=\"movie-detail__back\">\n                  <img src={back} alt=\"\"/>\n                </Link>\n                <div className=\"movie-detail__poster\">\n                  <img src={detailInfo.poster} loading=\"lazy\"/>\n                  <div className=\"movie-detail__spotify\">\n                    <ReactPlayer volume={volumePosterTrailer} loop={true} playing={playingPoster} url={detailInfo.music} loading=\"lazy\"/>\n                  </div>\n                </div>\n              </div>\n              <div className=\"col-md-9\">\n                  <div className=\"movie-detail__info text-white p-3\">\n                    <div className=\"d-flex flex-column flex-md-row align-items-start align-items-md-center justify-content-between\">\n                      <h2 className=\"movie-detail__title\">{detailInfo.name} <span>({detailInfo.year})</span></h2>\n                        {detailInfo.platform==\"games\" && \n                          <p className=\"movie-add green mt-2 md-md-0 mb-0 mb-md-3\" onClick={() => {setIsEdit(true); bodyAddClass()}}>Oyunu Düzenle</p>\n                        }\n                        {detailInfo.platform==\"movies\" && \n                          <p className=\"movie-add yellow mt-2 md-md-0 mb-0 mb-md-3\" onClick={() =>{setIsEdit(true); bodyAddClass()}}>Filmi Düzenle</p>\n                        }\n                    </div>\n                    <div className=\"d-flex group mt-2\">\n                      {detailInfo.company && \n                        <p className=\"movie-detail__top mr-2 mr-md-3\">{detailInfo.company} </p>\n                      }\n                      {detailInfo.director && \n                        <p className=\"movie-detail__top mr-3\">{detailInfo.director} </p>\n                      }\n                      {detailInfo.writer && \n                        <p className=\"movie-detail__top mr-3\">{detailInfo.writer} </p>\n                      }\n                      <p className=\"movie-detail__top \">{detailInfo.category} </p>\n                      <p className=\"movie-detail__top ml-3 d-none d-md-flex\">{detailInfo.time} </p>\n                    </div>\n                    <div className=\"d-flex movie-detail__center align-items-center mt-3\">\n                      <CircularProgressbar value={detailInfo.point} text={`${detailInfo.point}%`} strokeWidth={6} background\n                        styles={buildStyles({\n                          textSize: '30px',\n                          backgroundColor: \"#000\",\n                          textColor: '#fff',\n                          trailColor: '#0c502f',\n                      })} />\n                      <div className={`movie-detail__icon ml-3 cursor-pointer ${isMute ? \"opacity\" : \"\"}`} onClick={toggleMute}>\n                        <img src={muted} alt=\"\"/>\n                      </div>\n                      <div className=\"d-flex align-items-center cursor-pointer\" onClick={() => {\n                          setPlayPoster(false); \n                          setTrailer(true); \n                          setTrailerMuted(false); \n                          bodyAddClass();\n                        }}>\n                        <div className=\"movie-detail__icon ml-3 mr-2\">\n                          <img src={play} alt=\"\"/>\n                        </div>\n                        <p>Fragmana geç</p>\n                      </div>\n                    </div>\n                    <div className=\"movie-detail__bottom mt-5\">\n                      <h5 className=\"movie-detail__sum-title mb-2\">Özet</h5>\n                      <p className=\"movie-detail__summary\">{detailInfo.title} </p>\n                    </div>\n                  </div>\n                </div>\n            </div>\n          </div>\n          }\n          <div className={`movie-detail__edit ${isEdit ? \"active\" : \"\"}`}>\n             <img className=\"back-icon\" src={close} onClick={() => {setIsEdit(false);  bodyRemoveClass()}}/>\n              {isEdit && detailInfo.platform == \"movies\" &&\n                <EditMovie detailInfos = {detailInfo}/>\n              }\n              {isEdit && detailInfo.platform == \"games\" &&\n                <EditGames detailInfos = {detailInfo}/>\n              }\n          </div>\n      </div>\n    </div>\n    \n    <div className=\"back-info\" onClick={() => {\n         setPlayPoster(true);\n         isMutePoster(); \n         setTrailer(false); \n         setTrailerMuted(true);\n         bodyRemoveClass()\n      }}>\n      <img src={up} alt=\"\"/>\n    </div>\n  </div>\n  )\n}\nexport default MovieDetail;","/Users/sahinzaybak/Desktop/react-movie-app/src/components/createMovie.jsx",["127","128","129"],"import React, {  Component } from 'react'\nimport {connect} from 'react-redux' //redux ile bağlantı kurmak için connect gerekli.\n\n//Actions\nimport {saveMovie} from '../actions/movies'\n\n//Class component\nclass CreateMovie extends Component {\n  render(){ \n    return (\n      <div>\n        <div className=\"movie-detail__backdrop\">\n          <img src=\"https://images.flickreel.com/wp-content/uploads/2016/09/mad-max-fury-road.jpg\" loading=\"lazy\"/>\n        </div>\n        <div className=\"custom-container\">\n            <h4 className=\"mb-3 text-white\">Film Ekle</h4>\n            <form onSubmit={this.handleSubmit}>\n              <div className=\"movie-create__form movie d-flex flex-column\">\n                <div className=\"movie-create__item d-flex justify-content-between\">\n                <div className=\"d-flex flex-column w-100 mr-3\">\n                  <p>Film Adı</p>\n                  <input type=\"text\" onChange={this.handleTextChanged} name=\"name\" value={this.state.name} placeholder=\"Film Adı\" required/>\n                </div>\n                <div className=\"d-flex flex-column w-100 mr-3\">\n                  <p>Slug</p>\n                  <input type=\"text\" onChange={this.handleTextChanged} name=\"slug\" value={this.state.slug} placeholder=\"Slug\" required />\n                </div>\n                <div className=\"d-flex flex-column w-100\">\n                  <p>Kategori</p>\n                  <input type=\"text\" onChange={this.handleTextChanged} name=\"category\" value={this.state.category} placeholder=\"Kategori\" required/>\n                </div>\n                 \n                </div>\n                <div className=\"movie-create__item d-flex justify-content-between\">\n                  <div className=\"d-flex flex-column w-100 mr-3\">\n                    <p>Puan</p>\n                    <input type=\"text\" onChange={this.handleTextChanged} name=\"point\" value={this.state.point} placeholder=\"Puan\"  className=\"mr-0\" required/>\n                  </div>\n                  <div className=\"d-flex flex-column w-100 mr-3\">\n                    <p>Poster URL</p>\n                    <input type=\"text\" onChange={this.handleTextChanged} name=\"poster\" value={this.state.poster} placeholder=\"Poster URL\" required/>\n                  </div>\n                  <div className=\"d-flex flex-column w-100\">\n                    <p>Müzik URL (Youtube)</p>\n                    <input type=\"text\" onChange={this.handleTextChanged} name=\"music\"  value={this.state.music} placeholder=\"Müzik URL (Youtube)\" required/>\n                  </div>\n                </div>\n                <div className=\"movie-create__item d-flex justify-content-between\">\n                  <div className=\"d-flex flex-column w-100 mr-3\">\n                    <p>Yönetmen</p>\n                    <input type=\"text\" onChange={this.handleTextChanged} name=\"director\" value={this.state.director} placeholder=\"Yönetmen\" required/>\n                  </div>\n                  <div className=\"d-flex flex-column w-100 mr-3\">\n                    <p>Senaryo</p>\n                   <input type=\"text\" onChange={this.handleTextChanged} name=\"writer\" value={this.state.writer} placeholder=\"Senaryo\" required/>\n                  </div>\n                  <div className=\"d-flex flex-column w-100 mr-3\">\n                    <p>Fragman URL </p>\n                    <input type=\"text\" onChange={this.handleTextChanged} className=\"mr-5\" name=\"trailer\" value={this.state.trailer} placeholder=\"Fragman URL (Youtube)\" required/>\n                  </div>\n                  <div className=\"d-flex flex-column w-100\">\n                    <p>Film Süresi</p>\n                    <input type=\"text\" onChange={this.handleTextChanged} className=\"mr-0\" name=\"time\" value={this.state.time} placeholder=\"Film Süresi\" required/>\n                  </div>\n                </div>\n                <div className=\"movie-create__item d-flex justify-content-between\">\n                  <div className=\"d-flex flex-column w-100\">\n                    <p>Çıkış Tarihi</p>\n                    <input type=\"text\" onChange={this.handleTextChanged} name=\"year\" value={this.state.year}placeholder=\"Çıkış Tarihi\" className=\"mr-0\" required/>\n                  </div>\n                </div>\n                <div className=\"movie-create__item d-flex justify-content-between\">\n                  <div className=\"d-flex flex-column w-100\">\n                  <p>Film Konusu</p>\n                    <textarea rows=\"4\" type=\"text\" onChange={this.handleTextChanged} name=\"title\" value={this.state.title} placeholder=\"Film Detayı\" required/>\n                  </div>\n                </div>\n                  <input className=\"movie-create__button yellow\" type=\"submit\" value=\"Kaydet\" />\n                  {this.props.createResult==\"Created\" && this.state.message &&\n                    <h4 className=\"text-center status-message yellow\"> Yeni film girildi.</h4>\n                  }\n                  {!this.props.createResult==\"Created\" && this.state.message &&\n                    <h4 className=\"text-center status-message\"> Film ekleme sırasında hata oluştu !</h4>\n                  }\n                </div>\n            </form>\n        </div>\n      </div>\n    )\n  }\n\n  state = {\n    name:'',\n    title:'',\n    category:'',\n    slug:'',\n    poster:'',\n    director: '',\n    writer: '',\n    trailer:'',\n    music:'',\n    time:'',\n    year:'',\n    point:'',\n    message:true\n  }\n\n  componentDidMount(){\n    this.setState({message:false})\n  }\n\n  handleTextChanged = (e) => { //bunu yazmazsak inputa yazı giremeyiz..\n    this.setState({ [e.target.name]: e.target.value });\n  }\n\n handleSubmit = (e) => {\n    e.preventDefault();\n    this.props.saveMovie({\n      _name:this.state.name,\n      _title:this.state.title,\n      _category:this.state.category,\n      _slug:this.state.slug,\n      _poster:this.state.poster,\n      _music:this.state.music,\n      _director:this.state.director,\n      _writer:this.state.writer,\n      _trailer:this.state.trailer,\n      _time:this.state.time,\n      _year:this.state.year,\n      _point:this.state.point,\n    });\n    this.setState({message:true})\n    setTimeout(() => {\n      this.setState({message:false})\n    }, 2000);\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    createResult: state.movies.createMovieResult\n  } \n}\n\nconst mapDispatchToProps = {\n  saveMovie,\n}\n\nexport default connect(mapStateToProps,mapDispatchToProps) (CreateMovie);\n","/Users/sahinzaybak/Desktop/react-movie-app/src/pages/productEdit.jsx",["130","131"],"import React, { PureComponent } from 'react'\n\n//Components\nimport {connect} from 'react-redux' //redux ile bağlantı kurmak için connect gerekli.\nimport EditGame from '../components/editGame'\nimport EditMovie from '../components/editMovie'\nimport {detail} from '../actions/games'\n\n//Class component\nclass productEdit extends PureComponent {\n  render() {\n    return (\n      <div className=\"movie-create edit pt-5\">\n        {this.props.detailInfo.platform == \"games\" && \n          <EditGame detailInfo= {this.props.detailInfo}/>\n        }\n\n        {this.props.detailInfo.platform == \"movies\" && \n          <EditMovie detailInfo= {this.props.detailInfo}/>\n        }\n      </div>\n    )\n  }\n \n  componentDidMount(){ //Created\n    document.body.classList.add(\"header-nobg\");\n    const platform = this.props.match.params.platform //params platform\n    const slug = this.props.match.params.slug //params id\n    this.props.detail(platform,slug); //actiona gameId ve platform adı gönderdik.\n }\n}\n\nconst mapStateToProps = (state) => { //state 